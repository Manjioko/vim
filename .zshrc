#安装命令
#设置系统语言，这个设置很重要，如果没有设置，打开x环境后系统语言将默认是英文
#输入法安装后设置
if [ -n "$WINDOWID" ]; then
    TRANSPARENCY_HEX=$(printf 0x%x $((0xffffffff * 90/ 100)))
    xprop -id "$WINDOWID" -f _NET_WM_WINDOW_OPACITY 32c -set _NET_WM_WINDOW_OPACITY "$TRANSPARENCY_HEX"
fi
